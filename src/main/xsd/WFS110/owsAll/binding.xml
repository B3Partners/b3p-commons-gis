<binding xmlns="http://www.castor.org/SourceGenerator/Binding" 
            xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
            defaultBinding="element">

<!--OWS--> 
<!--Constraint-->
   <elementBinding name="complexType:RequestMethodType/Constraint">
      <java-class name="Constraint_RequestMethodType"/>
   </elementBinding>
     <elementBinding name="/OperationsMetadata/Constraint">
      <java-class name="Constraint_OperationsMetadata"/>
   </elementBinding>
	<elementBinding name="/Operation/Constraint">
      <java-class name="Constraint_Operation"/>
   </elementBinding>
   <!--Parameter-->
   <elementBinding name="/Operation/Parameter">
      <java-class name="Parameter_Operation"/>
   </elementBinding>
   <elementBinding name="/OperationsMetadata/Parameter">
      <java-class name="Parameter_OperationsMetadata"/>
   </elementBinding>
</binding>